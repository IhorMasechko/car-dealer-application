{
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended"],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off",
    "no-alert": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-uses-react": "off"
  }
}
